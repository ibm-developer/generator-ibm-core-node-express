{
  "name": "generator-ibm-core-node-express",
  "version": "3.1.3",
  "description": "Yeoman generator for core node express application",
  "license": "Apache-2.0",
  "keywords": [
    "yeoman-generator"
  ],
  "files": [
    "app",
    "lib"
  ],
  "dependencies": {
    "chalk": "^2.4.2",
    "handlebars": "^4.1.2",
    "ibm-openapi-support": "^0.0.13",
